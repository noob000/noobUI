{
  "name": "@noob000/noobui",
  "author": "noob000",
  "version": "0.1.5",
  "private": false,
  "keywords": [
    "react",
    "ui-components"
  ],
  "dependencies": {
    "@types/node": "^16.7.13",
    "@types/react": "^18.0.0",
    "@types/react-dom": "^18.0.0",
    "react": "^18.1.0",
    "react-dom": "^18.1.0",
    "react-scripts": "5.0.1",
    "typescript": "^4.7.3",
    "web-vitals": "^2.1.0"
  },
  "scripts": {
    "start": "react-scripts start",
    "build": "react-scripts build",
    "test": "react-scripts test",
    "eject": "react-scripts eject",
    "storybook": "start-storybook -p 6006 -s public",
    "build-storybook": "build-storybook -s public",
    "build-lib": "rimraf ./lib && rollup --config rollup.config.js"
  },
  "eslintConfig": {
    "extends": [
      "react-app",
      "react-app/jest"
    ],
    "overrides": [
      {
        "files": [
          "**/*.stories.*"
        ],
        "rules": {
          "import/no-anonymous-default-export": "off"
        }
      }
    ]
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  },
  "devDependencies": {
    "@babel/core": "^7.18.5",
    "@babel/preset-env": "^7.18.2",
    "@babel/preset-react": "^7.17.12",
    "@babel/preset-typescript": "^7.17.12",
    "@noob000/noobui": "^0.1.3",
    "@rollup/plugin-babel": "^5.3.1",
    "@rollup/plugin-node-resolve": "^13.3.0",
    "@rollup/plugin-typescript": "^8.3.3",
    "@storybook/addon-actions": "^6.5.0",
    "@storybook/addon-essentials": "^6.5.0",
    "@storybook/addon-interactions": "^6.5.0",
    "@storybook/addon-links": "^6.5.0",
    "@storybook/builder-webpack5": "^6.5.0",
    "@storybook/manager-webpack5": "^6.5.0",
    "@storybook/node-logger": "^6.5.0",
    "@storybook/preset-create-react-app": "^4.1.0",
    "@storybook/react": "^6.5.0",
    "@storybook/testing-library": "^0.0.11",
    "@testing-library/jest-dom": "^5.14.1",
    "@testing-library/react": "^13.0.0",
    "@testing-library/user-event": "^13.2.1",
    "@types/jest": "^27.0.1",
    "babel-plugin-named-exports-order": "^0.0.2",
    "cz-customizable": "^6.3.0",
    "node-sass": "^7.0.1",
    "prop-types": "^15.8.1",
    "rimraf": "^3.0.2",
    "rollup": "^2.75.6",
    "rollup-plugin-css-porter": "^1.0.2",
    "rollup-plugin-dts": "^4.2.2",
    "rollup-plugin-embed-css": "^1.0.24",
    "rollup-plugin-typescript2": "^0.32.1",
    "tslib": "^2.4.0",
    "webpack": "^5.72.1"
  },
  "repository": {
    "url": "https://github.com/noob000/noobUI",
    "type": "git"
  },
  "files": [
    "lib"
  ],
  "config": {
    "commitizen": {
      "path": "./node_modules/cz-customizable"
    },
    "cz-customizable": {
      "config": "./.cz.config.js"
    }
  },
  "types": "lib/types"
}
